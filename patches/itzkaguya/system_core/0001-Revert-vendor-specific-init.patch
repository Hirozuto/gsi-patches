Subject: [PATCH] Revert vendor specific init

---
 init/Android.bp           |  1 -
 init/property_service.cpp |  4 ----
 init/vendor_init.cpp      | 36 ------------------------------------
 init/vendor_init.h        | 32 --------------------------------
 4 files changed, 73 deletions(-)

diff --git a/init/Android.bp b/init/Android.bp
index a774f427a..0ab620cfa 100644
--- a/init/Android.bp
+++ b/init/Android.bp
@@ -136,7 +136,6 @@ cc_library_static {
         "ueventd.cpp",
         "ueventd_parser.cpp",
         "util.cpp",
-        "vendor_init.cpp",
     ],
     whole_static_libs: ["libcap", "com.android.sysprop.apex"],
     header_libs: ["bootimg_headers"],
diff --git a/init/property_service.cpp b/init/property_service.cpp
index 20685ea47..e1114ed0b 100644
--- a/init/property_service.cpp
+++ b/init/property_service.cpp
@@ -69,7 +69,6 @@
 #include "subcontext.h"
 #include "system/core/init/property_service.pb.h"
 #include "util.h"
-#include "vendor_init.h"
 
 using namespace std::literals;
 
@@ -982,9 +981,6 @@ void property_load_boot_defaults(bool load_debug_prop) {
     // Weaken property override security during execution of the vendor init extension
     weaken_prop_override_security = true;
 
-    // Update with vendor-specific property runtime overrides
-    vendor_load_properties();
-
     property_initialize_ro_product_props();
     property_derive_build_fingerprint();
 
